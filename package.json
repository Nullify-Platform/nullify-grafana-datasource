{
  "name": "nullify-grafana-datasource",
  "version": "0.0.0",
  "description": "Grafana datasource for the Nullify API",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Nullify",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nullify-platform/nullify-grafana-datasource/issues",
    "email": "support@nullify.ai"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "11.0.7",
    "@grafana/e2e-selectors": "11.3.0",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "16.0.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react-router-dom": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "copy-webpack-plugin": "^12.0.0",
    "css-loader": "^7.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.80.4",
    "sass-loader": "16.0.2",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.6.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "11.13.4",
    "@grafana/data": "11.3.0",
    "@grafana/runtime": "11.3.0",
    "@grafana/schema": "11.3.0",
    "@grafana/ui": "11.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "2.8.0",
    "zod": "^3.22.4"
  },
  "packageManager": "npm@10.9.0"
}
